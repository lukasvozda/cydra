type Error = variant {
    InvalidCanister;
    CanisterError : record {
        message : text;
    };
};

type Result = variant {
    Ok: text;
    Err : Error;
};

type QueryResult = variant {
    Ok: vec vec text;
    Err : Error;
};

service : {
    "balance": () -> (nat64);
    "instruction_counter": () -> (nat64);
    "execute": (text) -> (Result);
    "query": (text) -> (QueryResult) query;
}